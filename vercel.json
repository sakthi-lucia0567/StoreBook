{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Frontend/build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Backend/.*",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/build/index.html"
    }
  ]
}
